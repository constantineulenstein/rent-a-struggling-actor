<div class="container">
<h1>Users</h1>


<% @users.each do |user| %>
  <div class="border-bottom">
    <%= link_to user_path(user.id) do %>
    <ul>
      <li><%= user.first_name %></li>
      <li><%= user.last_name %></li>
      <li><%= user.location %></li>
      <li><%= user.username %></li>
    </ul>
    <% end %>
  </div>
<% end %>


</div>

